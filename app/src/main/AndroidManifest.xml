<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="be.huffle.rockout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_rock_out"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_rock_out_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".activities.AddTimeScheduleActivity"
            android:label="@string/add_time_schedule"/>
        <activity
            android:name=".activities.AddGymActivity"
            android:label="@string/add_gym"
            android:parentActivityName=".activities.GymListActivity" />
        <activity
            android:name=".activities.GymListActivity"
            android:label="Rock Out">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>